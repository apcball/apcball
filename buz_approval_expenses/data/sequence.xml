<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Expense Sheet -->
        <record id="sequence_expense_sheet" model="ir.sequence">
            <field name="name">Expense Sheet Sequence</field>
            <field name="code">hr.expense.sheet</field>
            <field name="prefix">EXP/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>