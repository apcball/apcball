<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Job Cost Sheet Sequence -->
        <record id="seq_job_cost_sheet" model="ir.sequence">
            <field name="name">Job Cost Sheet</field>
            <field name="code">job.cost.sheet</field>
            <field name="prefix">JCS/</field>
            <field name="suffix">/%(year)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Material Requisition Sequence -->
        <record id="seq_material_requisition" model="ir.sequence">
            <field name="name">Material Requisition</field>
            <field name="code">material.requisition</field>
            <field name="prefix">MR/</field>
            <field name="suffix">/%(year)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Job Order Sequence -->
        <record id="seq_job_order" model="ir.sequence">
            <field name="name">Job Order</field>
            <field name="code">job.order</field>
            <field name="prefix">JO/</field>
            <field name="suffix">/%(year)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
